$TTL	604800
{{ startup }}.	IN	SOA	agressive-nicoleta-2.{{ startup }}. nicoleta.{{ startup }}. (
			      2		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
{% for vm in groups['dns_servers']%}
{{ startup }}.       IN      NS      {{ vm }} 
{% endfor %}
{% for vm in groups['all'] %}
{{ vm }}	IN	A	{{ hostvars[vm]['ansible_default_ipv4']['address'] }}
{% endfor %}

