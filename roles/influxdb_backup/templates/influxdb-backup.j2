39 13 * * * backup rm -rf /home/backup/influxdb/*; influxd backup -portable -database telegraf /home/backup/influxdb
40 13 * * 0 backup duplicity --no-encryption full /home/backup/influxdb/ rsync://{{user}}@backup.{{startup}}//home/{{user}}/influxdb/
40 13 * * 1-6 backup duplicity --no-encryption incremental /home/backup/influxdb/ rsync://{{user}}@backup.{{startup}}//home/{{user}}/influxdb/